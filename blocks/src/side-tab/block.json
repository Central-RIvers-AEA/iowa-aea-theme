{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "iowa-aea-theme/side-tab",
	"version": "0.1.0",
	"title": "Side Tab",
	"category": "widgets",
	"icon": "smiley",
	"parent": ["iowa-aea-theme/side-tabs"],
	"attributes": {
		"tabNumber": {
			"type": "number",
			"default": 1
		},
		"backgroundColor": {
			"type": "string",
			"default": "#f28b82"
		},
		"textColor": {
			"type": "string",
			"default": "#000000"
		}
	},
	"description": "An individual tab block for use within the Side Tabs container.",
	"example": {},
	"supports": {
		"reusable": false
	},
	"textdomain": "side-tab",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
